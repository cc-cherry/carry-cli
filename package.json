{
  "name": "carry-cli",
  "version": "0.1.0",
  "description": "一个项目配置文件管理工具,也是一个模板管理脚手架",
  "author": "cc-chery",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:cc-cherry/carry-cli.git"
  },
  "keywords": [
    "cli",
    "config manager",
    "template manager"
  ],
  "main": "lib/index.js",
  "bin": {
    "cy": "lib/index.js"
  },
  "scripts": {
    "dev": "node src/index.js",
    "build": "webpack",
    "test": "node src/test.js",
    "commit": "cz",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "chalk": "4.1.2",
    "commander": "^10.0.1",
    "detect-package-manager": "^2.0.1",
    "download-git-repo": "^3.0.2",
    "edit-json-file": "^1.7.0",
    "execa": "5.1.1",
    "figlet": "^1.6.0",
    "fs-extra": "^11.1.1",
    "inquirer": "8.2.5",
    "metalsmith-filter": "^1.0.2",
    "ora": "5.4.1",
    "vm2": "^3.9.17"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.5",
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "babel-loader": "^9.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "commitizen": "^4.3.0",
    "consolidate": "^0.16.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.39.0",
    "handlebars": "^4.7.7",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.2",
    "metalsmith": "^2.5.1",
    "webpack": "^5.82.0",
    "webpack-cli": "^5.1.0",
    "webpack-node-externals": "^3.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
